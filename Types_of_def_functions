#1. a function with arguments and with return type
def login_credentials(uid,pwd):

    ruid=11445566
    rpwd="deekshitha@123"
    if(uid==ruid and pwd==rpwd):
        return "login successful"
    else:
        return "wrong credentials"
#2. a function with arguments and without return type
def login_credentials2(uid,pwd):

    ruid=11445566
    rpwd="deekshitha@123"
    if(uid==ruid and pwd==rpwd):
        print("login successful")
    else:
        print("wrong credentials")

#3. a function without arguments with return type
def system_properties_info():
    return "system RAM: 8 GB"

#4. a function without arguments and without return type
def system_properties_info2():
    print("system RAM: 8 GB")
    print("system OS:windows 11")
    print("system SSD: 512 GB")
